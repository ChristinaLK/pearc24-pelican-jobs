executable = sample-exec.sh
arguments = $(Cluster)
# the cluster variable is the first part of
# a job's ID number

transfer_input_files = input.txt
should_transfer_files = YES

log = job.$(Cluster).log
error = job.$(Cluster).error
output = job.$(Cluster).output

request_cpus = 1
request_memory = 1GB
request_disk = 1GB

+JobDurationCategory = "Medium"

# Queue one job with the above specifications.
queue 1
